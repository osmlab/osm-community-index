{
  "include": ["./src/oci.ts"],
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "declaration": true,
    "declarationDir": "./dist/ts",
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "outDir": "./dist/ts",
    "resolveJsonModule": true,
    "strict": true,
    "target": "ESNext"
  }
}
