{
  "name": "osm-community-index",
  "version": "2.1.1",
  "license": "ISC",
  "repository": "github:osmlab/osm-community-index",
  "author": "Bryan Housel <bhousel@gmail.com>",
  "description": "An index of community resources for OpenStreetMap",
  "keywords": [
    "OSM",
    "OpenStreetMap",
    "community"
  ],
  "main": "dist/index.js",
  "module": "index.mjs",
  "scripts": {
    "all": "run-s build dist",
    "build": "node build.js && rollup -c",
    "dist": "node build_dist.js",
    "icons": "node build_icons.js",
    "lint": "eslint *.js test/*.js",
    "test": "npm run build && npm run lint && tap test/*.js",
    "stats": "node stats.js",
    "txpull": "tx pull -a",
    "txpush": "tx push -s"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@ideditor/location-conflation": "0.5.0",
    "@mapbox/geojson-area": "^0.2.2",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "bytes": "^3.1.0",
    "colors": "^1.4.0",
    "easy-table": "^1.1.1",
    "eslint": "^7.11.0",
    "geojson-precision": "^1.0.0",
    "geojson-rewind": "^0.3.1",
    "glob": "^7.1.6",
    "js-yaml": "^3.14.0",
    "json-stringify-pretty-compact": "^2.0.0",
    "jsonschema": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.32.1",
    "shelljs": "^0.8.4",
    "tap": "^14.10.8"
  },
  "engines": {
    "node": ">=10"
  }
}
