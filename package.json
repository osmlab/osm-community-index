    "@mapbox/geojson-area": "^0.2.2",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "bytes": "^3.0.0",
    "colors": "^1.1.2",
    "easy-table": "^1.1.1",
    "eslint": "^6.8.0",
    "geojson-precision": "^0.4.0",
    "geojson-rewind": "^0.3.1",
    "glob": "^7.1.5",
    "js-yaml": "^3.13.1",
    "json-stringify-pretty-compact": "^1.1.0",
